<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="TemplateImport.Tile"
	x:Name="Window"
	Title="Tile"
	Width="640" Height="480">

	<Grid x:Name="LayoutRoot">
		<Canvas x:Name="systemy_liczbowe" ClipToBounds="True" HorizontalAlignment="Left" Height="768" UseLayoutRounding="False" VerticalAlignment="Top" Width="1366">
			<Image x:Name="bg_copy_2" Height="768" Canvas.Left="0" Source="systemy liczbowe_Images\bg copy 2.png" Canvas.Top="0" Width="1366"/>
			<Canvas x:Name="Tiles" Height="768" Canvas.Left="0" Canvas.Top="0" Width="1366">
				<Image x:Name="Background" Height="768" Canvas.Left="0" Source="systemy liczbowe_Images\Background.png" Canvas.Top="0" Width="1366"/>
				<Canvas x:Name="Tile_1x1" Height="150" Canvas.Left="608" Canvas.Top="309" Width="150">
					<Image x:Name="bg" Height="150" Canvas.Left="0" Source="systemy liczbowe_Images\bg.png" Canvas.Top="0" Width="150"/>
					<Image x:Name="Shape_1" Height="79" Canvas.Left="21" Source="systemy liczbowe_Images\Shape 1.png" Canvas.Top="31" Width="111"/>
				</Canvas>
			</Canvas>
		</Canvas>
	</Grid>
</Window>